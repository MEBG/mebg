<?xml version="1.0" encoding="UTF-8"?>
<Response>
    <erl>
      out(Arg) ->
         try
            io:format("~p",[Arg]),
            {ok, From} = queryvar(Arg, "From"),
            {ok, Body} = queryvar(Arg, "Body"),
            rcvr ! {From, Body},
            {content, "text/xml", []}
         catch
            error:{badmatch,undefined} ->
               [{status, 400},
               {html, "Query variables From and Body expected."}]
         end.
    </erl>
</Response>
